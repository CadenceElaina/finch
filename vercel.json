{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "tsc && vite build",
  "outputDirectory": "dist",
  "framework": "vite",
  "crons": [
    {
      "path": "/api/cron/morning-snapshot",
      "schedule": "0 13 * * 1-5"
    }
  ],
  "rewrites": [
    {
      "source": "/api/yh-finance",
      "destination": "/api/yh-finance"
    },
    {
      "source": "/api/seeking-alpha",
      "destination": "/api/seeking-alpha"
    }
  ]
}
